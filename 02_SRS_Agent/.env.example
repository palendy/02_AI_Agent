# SRS Generation Agent Environment Variables
# Copy this file to .env and fill in your API keys

# OpenAI Configuration (required for OpenAI models and embeddings)
OPENAI_API_KEY=your_openai_api_key_here
OPENAI_ORGANIZATION=your_openai_organization_id_here

# Anthropic Configuration (required for Claude models)
ANTHROPIC_API_KEY=your_anthropic_api_key_here

# General Configuration
DEBUG=false
LOG_LEVEL=INFO

# Model Selection Examples:
# For OpenAI: gpt-4o, gpt-4o-mini, gpt-3.5-turbo
# For Claude: claude-3-5-sonnet-20241022, claude-3-haiku-20240307, claude-3-opus-20240229